<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:layout_marginTop="@dimen/dp_18"
    android:paddingLeft="@dimen/dp_20"
    android:paddingRight="@dimen/dp_20"
    tools:ignore="MissingConstraints">

    <ImageView
        android:id="@+id/headIv"
        android:layout_width="@dimen/dp_46"
        android:layout_height="@dimen/dp_46"
        android:src="@mipmap/icon_null" />

    <TextView
        android:id="@+id/nameTv"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="@dimen/dp_15"
        android:textColor="@color/c_001A2B"
        android:textSize="@dimen/sp_15"
        app:layout_constraintLeft_toRightOf="@+id/headIv" />

    <TextView
        android:id="@+id/tagTv"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="@dimen/dp_12"
        android:background="@drawable/shape_white_0071bc_7"
        android:paddingLeft="@dimen/dp_6"
        android:paddingTop="@dimen/dp_1"
        android:paddingRight="@dimen/dp_6"
        android:paddingBottom="@dimen/dp_1"
        android:textColor="@color/c_0071BC"
        android:textSize="@dimen/sp_10"
        app:layout_constraintBottom_toBottomOf="@+id/nameTv"
        app:layout_constraintLeft_toRightOf="@+id/nameTv"
        app:layout_constraintTop_toTopOf="@+id/nameTv" />

    <TextView
        android:id="@+id/oneselfTv"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="@dimen/dp_6"
        android:background="@drawable/shape_3480ff_7"
        android:paddingLeft="@dimen/dp_6"
        android:paddingTop="@dimen/dp_1"
        android:visibility="gone"
        android:paddingRight="@dimen/dp_6"
        android:paddingBottom="@dimen/dp_1"
        android:text="@string/oneself"
        android:textColor="@color/white"
        android:textSize="@dimen/sp_10"
        app:layout_constraintBottom_toBottomOf="@+id/nameTv"
        app:layout_constraintLeft_toRightOf="@+id/tagTv"
        app:layout_constraintTop_toTopOf="@+id/nameTv" />

    <TextView
        android:id="@+id/companyTv"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="@dimen/dp_15"
        android:textColor="@color/c_8C9AA3"
        android:textSize="@dimen/sp_13"
        app:layout_constraintBottom_toBottomOf="@+id/headIv"
        app:layout_constraintLeft_toRightOf="@+id/headIv" />

    <TextView
        android:id="@+id/roleTv"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="@dimen/dp_15"
        android:textColor="@color/c_0071BC"
        android:textSize="@dimen/sp_14"
        app:layout_constraintBottom_toBottomOf="@+id/headIv"
        app:layout_constraintRight_toLeftOf="@+id/iv"
        app:layout_constraintTop_toTopOf="@+id/headIv" />

    <ImageView
        android:id="@+id/iv"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:paddingLeft="@dimen/dp_8"
        android:src="@mipmap/icon_set"
        app:layout_constraintBottom_toBottomOf="@+id/headIv"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="@+id/headIv" />

</androidx.constraintlayout.widget.ConstraintLayout>